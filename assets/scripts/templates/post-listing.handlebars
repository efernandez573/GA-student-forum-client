{{#each posts as |post|}}
<h1> </h1>

    <p class="card-text"></p>

  <div class="card mb-3">
    <img class="card-img-top" src="" alt="Card image cap">
    <div class="card-body">
      <h5 class="card-title">{{post.title}}</h5>
      <p class="card-text">{{post.link}}</p>
      <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
    </div>
</div>
{{/each}}
